{
    "rules": {
        "no-unreachable": true,
        "no-duplicate-variable": true,
        "no-duplicate-key": true,
        "arrow-parens": true,
        "prefer-const": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true
    },
    "defaultSeverity": "warning"
}